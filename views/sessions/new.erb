<h2>Create a new class</h2>
<form method="POST" action="/sessions">
  <div class="form-wrapper">

    <div class="form-el">
      <label for="name">Name of the class:</label>
      <input type="text" name="name" id="name" required>
    </div>

    <div class="form-el">
      <label for="time">Time:</label>
      <input type="time" name="time" id="time" reuired>
    </div>

    <label>
       Select a name:
      <select name="human_id">
        <% for human in @humans %>
        <option value="<%= human.id %>"><%= human.first_name %></option>
        <% end %>
      </select>
    </label>

      <input type="submit" value="Submit new class">
    
  </div>

</form>
